<?xml version="1.0" encoding="utf-8" ?>
<go-plugin id="artifactory-scm" version="1">
<about>
    <name>Artifactory SCM Plugin</name>
    <version>@version@</version>
    <target-go-version>@goTargetVersion@</target-go-version>
    <description>Able to download several versions of an artifact from Artifactory repository</description>
    <vendor>
        <name>Christoph Nenning</name>
        <url>https://github.com/cnenning/go-artifactory-scm-plugin/README.md</url>
    </vendor>
</about>
</go-plugin>
